# Environment Variables Example
# Copy this file to .env and update with your values

# Application
NODE_ENV=development
PORT=3000

# Database
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_USER=galeri_user
POSTGRES_PASSWORD=galeri_password
POSTGRES_DB=galeri_db
POSTGRES_MAX_CONNECTIONS=20

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# RabbitMQ
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=galeri_user
RABBITMQ_PASSWORD=galeri_password
RABBITMQ_VHOST=galeri

# JWT
JWT_SECRET=change_this_secret_in_production
JWT_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# SMS Provider (NetGSM)
SMS_PROVIDER=netgsm
NETGSM_USERNAME=
NETGSM_PASSWORD=
NETGSM_MSG_HEADER=GALERIPLATFORM

# Email (SMTP)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
SMTP_FROM=noreply@domain.com

# MinIO (S3-compatible storage)
MINIO_ENDPOINT=minio
MINIO_PORT=9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=galeri-media
MINIO_USE_SSL=false

# Meilisearch
MEILISEARCH_HOST=http://meilisearch:7700
MEILISEARCH_MASTER_KEY=master_key_change_in_production

# EİDS Integration
EIDS_API_URL=
EIDS_CLIENT_ID=
EIDS_CLIENT_SECRET=
EIDS_CALLBACK_URL=

# CORS
CORS_ORIGIN=http://localhost:3000,http://localhost:3001,http://localhost:3002

# Frontend URLs
LANDING_URL=http://localhost:3000
ADMIN_URL=http://localhost:3001
PANEL_URL=http://localhost:3002

# WebSocket
WS_URL=ws://localhost:3005
